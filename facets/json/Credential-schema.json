            "readOnly": false,
            "versionable": false,
            "id": "CE_Credential_authenticationType",
            "title": "Credential.authenticationType",
            "enum": [
                "BASIC_HTTP",
                "OAUTH2",
                "SSH",
                "HEADER"
            ]
        },
        "headerKey": {
            "storages": ["SQL"],
            "default": "X-API-KEY",
            "nullable": true,
            "description": "Header key EL",
            "readOnly": false,
            "versionable": false,
            "id": "CE_Credential_headerKey",
            "title": "Credential.headerKey",
            "type": "string",
            "maxLength": 50
        },
        "lastConnection": {
            "storages": ["SQL"],
            "nullable": true,
            "format": "date-time",
            "description": "Last connection",
            "readOnly": false,
            "versionable": false,
            "id": "CE_Credential_lastConnection",
            "title": "Credential.lastConnection",
            "type": "string"
