        "apiKey": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Api key",
            "readOnly": false,
            "versionable": false,
            "id": "CE_Credential_apiKey",
            "title": "Credential.apiKey",
            "type": "string",
            "maxLength": 255
        },
        "passwordSecret": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Password Secret",
            "readOnly": false,
            "versionable": false,
            "id": "CE_Credential_passwordSecret",
            "title": "Credential.passwordSecret",
            "type": "string",
            "maxLength": 255
        },
        "headerValue": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Header value EL",
            "readOnly": false,
            "versionable": false,
            "id": "CE_Credential_headerValue",
            "title": "Credential.headerValue",
            "type": "string",
            "maxLength": 1000
